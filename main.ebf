main:
    .
    call fact result
    exit

add:
    .
    goto other
    .
    [
        -
        goto result
        +
        goto other
    ]
    exit

mul:
    0
    goto value1
    .
    goto value2
    .

    goto value1
    [
        goto result
        call add value2 result
        goto value1
        -
    ]
    exit

fact:
    .
    goto n
    result

    ![
        goto result
        1
        exit
    ]

    -
    call fact n

    goto result
    call mul n result
    exit